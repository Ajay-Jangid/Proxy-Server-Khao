{
    "version": 2,
    "builds": [
        {
            "src": "./server.js",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "https://khao-backend.vercel.app"
        },
        {
            "source": "/",
            "destination": "https://khao.vercel.app"
        }
    ]
}